{
  "name": "ultravox-client",
  "version": "0.2.9",
  "private": false,
  "main": "./dist/cjs/session.js",
  "module": "./dist/esm/session.js",
  "browser": "./dist/esm/session.js",
  "types": "./dist/esm/session.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/session.js",
      "require": "./dist/cjs/session.js",
      "types": "./dist/esm/session.d.ts"
    }
  },
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "pnpm run build:esm && pnpm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "livekit-client": "^2.0.10"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.1.3"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "engines": {
    "pnpm": ">=6.0.0"
  }
}